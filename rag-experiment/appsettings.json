{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "OpenAI": {
    "ApiKey": "",
    "EnableRateLimiting": false,
    "ChatModel": "gpt-3.5-turbo"
  },
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=app.db"
  },
  "RagConfiguration": {
    "TextProcessing": {
      "StopwordRemoval": false,
      "Stemming": false,
      "Lemmatization": false,
      "QueryExpansion": false
    },
    "Embedding": {
      "ModelName": "text-embedding-3-small",
      "Dimension": 1536
    },
    "Chunking": {
      "ChunkSize": 1000,
      "ChunkOverlap": 100
    },
    "Retrieval": {
      "DefaultTopK": 10
    }
  },
  "MarkdownTablePath": "docs/experiment_results.md",
  "CsvExportPath": "docs/experiment_results.csv"
}
